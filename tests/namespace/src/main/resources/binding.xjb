<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings
  version="3.0"
  xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"

  xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
  xmlns:namespace="http://jaxb3-commons.dev.java.net/basic/namespace-prefix"

  jaxb:extensionBindingPrefixes="xjc">

	<jaxb:bindings schemaLocation="a.xsd">
		<jaxb:schemaBindings>
			<jaxb:package name="org.jvnet.jaxb3_commons.test.namespace.a" />
		</jaxb:schemaBindings>
		<jaxb:bindings>
			<namespace:prefix name="a" />
		</jaxb:bindings>
	</jaxb:bindings>
	<jaxb:bindings schemaLocation="b.xsd">
		<jaxb:schemaBindings>
			<jaxb:package name="org.jvnet.jaxb3_commons.test.namespace.b" />
		</jaxb:schemaBindings>
		<jaxb:bindings>
			<namespace:prefix name="b" />
		</jaxb:bindings>
	</jaxb:bindings>
</jaxb:bindings>
