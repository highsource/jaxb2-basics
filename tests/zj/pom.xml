<project xmlns="http://maven.apache.org/POM/4.0.0" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.jvnet.jaxb3_commons</groupId>
		<artifactId>jaxb3-basics-tests</artifactId>
		<version>3.0.0-SNAPSHOT</version>
	</parent>
	<artifactId>jaxb3-basics-test-zj</artifactId>
	<packaging>jar</packaging>
	<name>JAXB3 Basics - Test [zj]</name>
	<build>
		<defaultGoal>test</defaultGoal>
		<plugins>
			<plugin>
				<groupId>org.jvnet.jaxb</groupId>
				<artifactId>jaxb3-maven-plugin</artifactId>
				<configuration>
					<args>
						<arg>-XtoString</arg>
						<arg>-Xequals</arg>
						<arg>-XhashCode</arg>
						<arg>-Xcopyable</arg>
					</args>
					<plugins>
                                        	<plugin>
                                                	<groupId>org.jvnet.jaxb3_commons</groupId>
							<artifactId>jaxb3-basics</artifactId>
						</plugin>
					</plugins>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
